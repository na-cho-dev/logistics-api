{
    "openapi": "3.0.0",
    "info": {
        "title": "Logistics API",
        "version": "1.0.0",
        "description": "API documentation for Logistics APP"
    },
    "paths": {
        "/api/v1/customers": {
            "get": {
                "summary": "Get all customers",
                "responses": {
                    "200": {
                        "description": "A list of customers",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "id": {
                                                "type": "integer",
                                                "example": 1
                                            },
                                            "name": {
                                                "type": "string",
                                                "example": "John Doe"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}